# Copy this file to .env and fill in your values

# Supabase Configuration
# Get these from https://app.supabase.com/project/your-project/settings/api
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# RevenueCat Configuration (for subscription payments)
# Get these from https://app.revenuecat.com
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=your_revenuecat_ios_key_here
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=your_revenuecat_android_key_here

# Superwall Configuration (for paywalls)
# Get this from https://superwall.com
EXPO_PUBLIC_SUPERWALL_API_KEY=your_superwall_api_key_here
